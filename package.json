        "@mui/icons-material": "^5.4.2",
        "@mui/material": "^5.4.2",
        "@mui/styles": "^5.4.2",
        "@next-auth/prisma-adapter": "^1.0.1",
        "@prisma/client": "^3.9.2",
        "axios": "^0.26.0",
        "gray-matter": "^4.0.3",
        "next": "^12.0.10",
        "next-auth": "^4.2.1",
        "next-pwa": "^5.4.4",
        "nodemailer": "^6.7.2",
        "nprogress": "^0.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.27.0",
