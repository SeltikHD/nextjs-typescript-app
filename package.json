        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "eslint": "<8.0.0",
        "eslint-config-next": "^12.0.10",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.4",
        "next-sitemap": "^2.4.2",
        "prettier": "^2.5.1",
